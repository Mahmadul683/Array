#include <stdio.h>
int a[1000];

int main() {

	int index, n;
	scanf("%d",&n);
for(int i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}
for(int i=0;i<n;i++)
{
    printf("%d \n",a[i]);
}
scanf("%d",&index);
for(int i=index;i<n-1;i++)
{a[i]=a[i+1];
}
for(int i=0;i<n-1;i++)
{
    printf("%d ",a[i]);
}
return 0;
}
